{
  "name": "express-boilerplate-js",
  "version": "0.0.1",
  "repository": "https://github.com/ZzoNe322/express-boilerplate-js",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node ./scripts/start.js",
    "test": "jest",
    "test:lint": "eslint --ignore-path .gitignore \"**/*.{js,jsx}\""
  },
  "jest": {
    "testEnvironment": "./scripts/test/test-environment.js"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@google-cloud/storage": "^2.5.0",
    "@google-cloud/vision": "^1.0.0",
    "colour-proximity": "0.0.2",
    "convert-csv-to-json": "0.0.14",
    "cookie-parser": "~1.4.3",
    "csvtojson": "^2.0.8",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "fast-csv": "^2.5.0",
    "http-errors": "~1.6.2",
    "joi": "^14.3.1",
    "node-fetch": "^2.6.0",
    "rgb-hex": "^3.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-plugin-jest": "^22.2.2",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "jest-environment-node": "^24.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.3"
  }
}
